<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_br_account_einvoice_invoice_form" model="ir.ui.view">
        <field name="name">br_nfse_dsf.account.invoice</field>
        <field name="model">account.invoice</field>
        <field name="inherit_id" ref="br_account.view_br_account_invoice_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='other_info']/group/group/field[@name='service_serie_id']" position="after">
                <field name="operation" />
                <field name="tributacao" />
                <field name="tipo_recolhimento" />
            </xpath>
        </field>
    </record>
</odoo>